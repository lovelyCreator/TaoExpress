{
  "expo": {
    "name": "TaoExpress",
    "slug": "taoexpress",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload images for product variations.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos for product variations."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.app.taoexpress",
      "networkSecurityConfig": "@xml/network_security_config",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "scheme": "taoexpress",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your photo library to upload images for product variations.",
          "cameraPermission": "This app needs access to your camera to take photos for product variations."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.taoexpress"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7344e826-9eff-42c4-9dbb-6525bb2f6c90"
      }
    }
  }
}